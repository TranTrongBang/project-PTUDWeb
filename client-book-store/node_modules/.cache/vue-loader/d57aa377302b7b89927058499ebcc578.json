);\r\n                }\r\n                return years;\r\n            })()\r\n        };\r\n    },\r\n    created() {\r\n        PublishingCompanyService.list({ tennxb: '' })\r\n            .then(\r\n                response => {\r\n                    this.publishingCompanys = response;\r\n                    console.log(response);\r\n                },\r\n                error => {\r\n                    return error;\r\n                }\r\n            );\r\n    },\r\n    methods: {\r\n        handleSave() {\r\n            this.isSaving = true\r\n            console.log(this.book);\r\n            BookService.create(this.book)\r\n                .then(response => {\r\n                    Swal.fire({\r\n                        icon: 'success',\r\n                        title: 'Sách được tạo thành công!',\r\n                        showConfirmButton: false,\r\n                        timer: 1500\r\n                    })\r\n                    this.isSaving = false\r\n                    this.book.tennxb = \"\"\r\n                    this.book.diachi = \"\"\r\n                    this.$router.push('/sach');\r\n                    return response\r\n                })\r\n                .catch(error => {\r\n                    this.isSaving = false\r\n                    Swal.fire({\r\n                        icon: 'error',\r\n                        title: 'Có lỗi đã xảy ra!',\r\n                        showConfirmButton: false,\r\n                        timer: 1500\r\n                    })\r\n                    return error\r\n                });\r\n        },\r\n    },\r\n};\r\n</script>"]}]}